	box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.5);
	display:flex;
}
.login
{
	margin:5px;
	position:absolute;
	right:20px;
	
}
.horizontal
{
	margin:5px;
	border-right : 2px solid black;
