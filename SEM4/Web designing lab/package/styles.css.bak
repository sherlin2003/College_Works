	height:135px;
	mix-blend-mode: multiply;
}	

a
{
	text-decoration:none;
	color:black;
}	
.image
{
	padding-top:1%;
	padding-left:1%;
	padding-right:1%;
	padding-bottom:1%;
	width:100%;
	height:69%;
}
.body2
{
	filter: drop-shadow(5px 5px 5px rgba(0, 0, 0, 0.5)); 
}
.mySlides 
{
	display:none;
}
.content1
{
	background-color:#1A132F;
}	
