	text-decoration:none;
}
li
{
    display: list-item;
    text-align: -webkit-match-parent;
}

.list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
}

.list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
}

.navbar
{
	margin-left:330px;
}
